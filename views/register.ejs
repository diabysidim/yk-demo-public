<% include partials/header1 %>

<title>Registration</title>
<link rel="icon" href="image/yk.PNG" />
<% include partials/header2 %>


<div class="register-container">

	<div class="register-form"> 


		<form action="/register" method="POST">
	    	<table>
	    	<tr>
	    		<td>Prenom: </td>
	    		<td><input name="name" placeholder="Prenom"></td>

	    	</tr>


	    	<tr>
	    		<td>Nom:</td>
	    		<td><input name="lastName"  placeholder="Nom"></td>

	    	</tr>


	    	<tr>
	    		<td>Username:</td>
	    		<td><input name="username" placeholder="Username"></td>

	    	</tr>


	    	<tr>
	    		<td> Mot De Passe:</td>
	    		<td><input name="password"  type="password" minlength="8" placeholder="Mot De Passe"></td>

	    	</tr>

	    	<tr>
	    		<td>Confirmation Du Mot de passe:</td>
	    		<td><input name="passwordConfirmation" type="password" minlength="8" placeholder="Confirmation Du Mot de passe"></td>

	    	</tr>		

	   
	    
	   </table>
	    
		
    
    	<button class="go-button" style="margin-left: 120px"> Registrer</button>

    	</form>


	</div>
	


    


</div>






<%include partials/footer%>